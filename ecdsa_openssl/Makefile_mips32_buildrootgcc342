CC = /opt/buildroot-gcc342/bin/mipsel-linux-gcc

CFLAGS = -g -Wall 

INCLUDES = -I/home/qwertyu/openssl_crosscompile_342/include

LFLAGS = -L/home/qwertyu/openssl_crosscompile_342/lib

LIBS = -lcrypto -lssl -lpthread -ldl

SRCS = ecdsa_main.c ecdsa_f.c 

OBJS = $(SRCS:.c=.o)

MAIN = ecdsa_test01_mips32_buildrootgcc342

all:	$(SRCS)
	$(CC) $(SRCS) $(CFLAGS) $(INCLUDES) $(LFLAGS) $(LIBS) -o $(MAIN) 

clean:
	$(RM) *.o *~ 



